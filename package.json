{
  "name": "@laomao800/vue-listview",
  "version": "1.0.0-alpha.19",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "release": "node scripts/release.js",
    "release:private": "node scripts/release-private.js",
    "release:gh-pages": "git subtree push --prefix=docs/.vuepress/dist origin gh-pages",
    "build": "rimraf dist&npm run build:component&npm run build:full",
    "build:component": "cross-env BUILD_MODE=component vue-cli-service build --no-clean --target lib --name listview-component src/index.component.js",
    "build:full": "vue-cli-service build --no-clean --target lib --name listview src/index.js",
    "docs:serve": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "storybook:serve": "vue-cli-service serve:storybook -p 6006",
    "storybook:build": "vue-cli-service build:storybook -o docs/.vuepress/dist/storybook",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch --notify --notifyMode change",
    "test:unit:cover": "vue-cli-service test:unit --coverage"
  },
  "author": "laomao800 <laomao800@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laomao800/vue-listview.git"
  },
  "bugs": {
    "url": "https://github.com/laomao800/vue-listview/issues"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.25",
    "@storybook/addon-links": "^4.0.0-alpha.25",
    "@storybook/addon-options": "^4.0.0-alpha.25",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-unit-jest": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-standard": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.24",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "conventional-changelog": "^2.0.3",
    "conventional-changelog-angular": "^5.0.1",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.9",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "execa": "^1.0.0",
    "express": "^4.16.3",
    "express-delay": "^0.2.0",
    "ftp-deploy": "^2.1.2",
    "get-value": "^3.0.1",
    "has-values": "^2.0.1",
    "html-loader": "^0.5.5",
    "inquirer": "^6.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "markdown-it-plantuml": "^1.2.0",
    "markdown-loader": "^3.0.0",
    "mockjs": "^1.0.1-beta3",
    "normalize.css": "^8.0.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.1",
    "storybook-readme": "^4.0.1",
    "stylelint": "^9.2.1",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-prettier": "^3.2.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "vue": "^2.5.17",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.2",
    "vuex": "^3.0.1"
  },
  "main": "dist/listview-component.common.js",
  "files": [
    "dist",
    "src"
  ],
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
